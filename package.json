{
  "name": "higa",
  "version": "0.1.0-fix",
  "description": "A package to interract with the Discord API coded by myself",
  "main": "build",
  "typings": "types",
  "scripts": {
    "build": "tsc",
    "dev:build": "tsc -w",
    "prettier": "prettier **.json src --write",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "dev": "nodemon --watch src --ext ts --exec npm run dev:serve",
    "dev:serve": "ts-node --files src/index.ts -- true"
  },
  "keywords": [
    "nodejs",
    "discord",
    "discord-bot",
    "discord-api"
  ],
  "dependencies": {
    "axios": "^0.26.0",
    "tmi.js": "^1.8.5",
    "firebase-admin": "^10.0.2"
  },
  "devDependencies": {
    "@types/tmi.js": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.12",
    "typescript": "4.5.5"
  },
  "contributors": [
    "fantomitechno <fantomitechno@gmail.com>"
  ],
  "author": "fantomitechno <fantomitechno@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}